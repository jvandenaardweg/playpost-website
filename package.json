  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "core-js": "^3.1.4",
    "gatsby": "2.11.0",
    "gatsby-image": "2.2.3",
    "gatsby-link": "2.2.0",
