    "gatsby-plugin-react-next": "2.0.1-13",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sass": "^2.0.0-rc.2",
    "gatsby-plugin-sharp": "2.2.1",
    "gatsby-plugin-sitemap": "2.2.0",
